<!--  sidebar  -->
<div class="sidebar fl oh">
	<div class="begining w100 oh clear">
    	<h1>管理目录<em>Contents</em></h1>
    </div>
    
    <!--  mainContent  -->
    <div class="mainContent clear">
    	
        <!--  栏目列表  -->
        <div class="columnList clear">
        	<ul>
        		<li class="userCenter"><span>用户中心<em></em></span>
                    <ul>
                    	<li><a id="checkZL">查看专利</a></li>
						<li><a href="#">申请审核表填写/上传</a></li>
						<li><a href="#">专利费用减缓证明</a></li>
        			</ul>
                </li>
        		<li class="personalSet"><span>个人设置<em></em></span>
                    <ul>
                    	<li><a href="#">查看专利</a></li>
						<li><a href="#">申请审核表填写/上传</a></li>
						<li><a href="#">专利费用减缓证明</a></li>
        			</ul>
                </li>
        		<li class="administrator"><span><a href="#">管理员<em></em></a></span></li>
        		<li class="announ"><span><a href="#">通知公告<em></em></a></span></li>
        		<li class="systemManage"><span><a href="#">系统管理<em></em></a></span></li>        
        	</ul>
        </div>
        <!--  栏目列表 end  -->
    
    
    </div>
    <!--  mainContent end  -->



</div>
<!--  sidebar end  -->


<!--  mainbody  -->
<div class="mainbody fr">
	<div class="begining w100 oh clear">
    	<p class="location">您的位置：<a href="#">首页</a>&nbsp;>&nbsp;<a href="#">用户中心</a>&nbsp;>&nbsp;申请审批</p>
    </div>
    
    <!--  mainContent  -->
   <div class="mainContent" id="mainvalue">
   
   
     <!--  申请审批表  -->
    <div class="formApply clear">
            
            <!--  进度条  -->
            <div class="progressBar middle tc clear oh">
                <em></em>
                <em></em>
                <ul>
                    <li class="on"><em>1</em><p>专利费用减缓申请审批表填写</p></li>
                    <li><em>2</em><p>导出申请审批表加盖公章</p></li>
                    <li><em>3</em><p>上传申请审批表</p></li>
                </ul>
            </div>
            <!--  进度条 end  -->
            
            <!--  第一步  -->
            <div class="stepOne clear middle">
                
                	<!--  填写表格  -->
                    <div class="formWrite clear">
                    	<table>
                    		<tr>
                            	<th valign="top"><span>申请单位名称：</span></th>
                                <td valign="top"><input type="text"></td>
                            	<th valign="top"><span>联系人：</span></th>
                                <td valign="top"><input type="text"></td>
                            </tr>
                    		<tr>
                            	<th valign="top"><span>地&nbsp;址：</span></th>
                                <td valign="top"><input type="text"></td>
                            	<th valign="top"><span>电&nbsp;话：</span></th>
                                <td valign="top"><input type="text"></td>
                            </tr>
                    		<tr>
                            	<th valign="top"><span>所属性质：</span></th>
                                <td valign="top" colspan="3">
                                	<label><input type="checkbox">企业</label>
                                	<label><input type="checkbox">事业</label>
                                	<label><input type="checkbox">高校</label>
                                	<label><input type="checkbox">科研机构</label>
                                	<label><input type="checkbox">机关团体</label>
                                	<label><input type="checkbox">个人</label>
                                </td>
                            </tr>
                    		<tr>
                            	<th valign="top"><span>申请理由：</span></th>
                                <td valign="top" colspan="3"><textarea rows="10"></textarea></td>
                            </tr>
                    	</table>
                    </div>
                    <!--  填写表格 end  -->
                    
                    <!--  申请专利证明  -->
                    <div class="applyProve w100 clear tc">
                        <h1>申请专利费用减缓证明项目（可附页）</h1>
                        <table>
                            <thead>
                                <tr>
                                    <td>专利申请类型</td>
                                    <td>专利申请名称</td>
                                    <td>______年编号</td>
                                </tr>
                            </thead>
                            <tbody>
                                <tr>
                                    <td><select><option>请选择</option></select></td>
                                    <td></td>
                                    <td></td>
                                </tr>
                            </tbody>
                        </table>
                        
                    </div>
                    <!--  申请专利项目 end  -->
            
            </div>
            <!--  第一步 end  -->
            
            
            <!--  第二步  -->
            <div class="stepTwo tc oh clear">
                <span></span>
                <span></span>
            </div>
            <!--  第二步 end  -->
            
            
            <!--  第三步  -->
            <div class="stepThree clear oh tc middle">
                <em>请选择要上传的文件</em>
                <input type="text" placeholder="费减申请表">
                <span class="upload"></span>
                <span class="submit"></span>
                <span class="cancel"></span>
            </div>
            <!--  第三步 end  -->
            
            
            
            
            
        </div>
        <!--  申请审批表 end  -->
   
   
   </div>
    <!--  mainContent end  -->



</div>
